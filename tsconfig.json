{
	"compilerOptions": {
    "charset": "utf8",
    "declaration": true,
    "declarationDir": ".",
    "downlevelIteration": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
		"lib": [
      "DOM",
      "ESNext",
    ],
    "module": "CommonJS",
		"moduleResolution": "node",
    "newLine": "lf",
    "outDir": "build",
    "pretty": true,
    "removeComments": true,
    "skipLibCheck": true,
		"target": "ES6",
	},
  "files": [
    "src/bin/generate.ts",
    "src/bin/solve.ts",
    "src/lib/aStar.ts",
    "src/lib/common.ts",
    "src/lib/coordinate.ts",
    "src/lib/coordinateUnit.ts",
    "src/lib/euclidean.ts",
    "src/lib/graph.ts",
    "src/lib/standard.ts",
    "src/lib/worker.ts",
    "src/tsgraph.ts",
  ],
}
