{
  "compilerOptions": {
    "charset": "utf8",
    "declaration": true,
    "declarationDir": ".",
    "downlevelIteration": true,
    "emitDeclarationOnly": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": [
      "DOM",
      "ESNext",
    ],
    "module": "CommonJS",
    "moduleResolution": "node",
    "newLine": "lf",
    "pretty": true,
    "removeComments": false,
    "skipLibCheck": true,
    "target": "ES2015",
  },
  "exclude": [],
  "include": [
    "src",
  ],
}
