  "dependencies": {
    "@kei-g/priority-queue": "^1.0.2",
    "canvas": "^2.8.0"
  },
  "description": "A star search algorithm implementation in TypeScript",
  "devDependencies": {
    "@types/node": "^16.3.0",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "eslint": "^7.30.0",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.5",
    "uglify-js": "^3.13.10"
  },
  "displayName": "tsgraph",
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/kei-g/tsgraph",
  "keywords": [
    "A*",
    "A star",
    "Graph",
    "TypeScript"
  ],
  "license": "BSD-3-Clause",
  "main": "dist/tsgraph.js",
  "name": "@kei-g/tsgraph",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kei-g/tsgraph.git"
  },
  "scripts": {
    "build": "tsc && [ -d dist/bin ] || mkdir -p dist/bin && [ -d dist/lib ] || mkdir dist/lib && cd build && find . -type f | while read name; do uglifyjs $name --compress --mangle > ../dist/$name; done",
    "example": "ts-node example.ts",
    "generate": "ts-node bin/generate.ts",
    "lint": "eslint bin/*.ts lib/*.ts *.ts",
    "prepublishOnly": "npm run build",
    "solve": "ts-node bin/solve.ts",
    "start": "ts-node bin/generate.ts && ts-node bin/solve.ts"
  },
  "version": "1.0.0"
}
