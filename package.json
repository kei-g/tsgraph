{
  "author": {
    "email": "km.8k6ce+github@gmail.com",
    "name": "kei-g"
  },
  "bin": {
    "tsgraph": "tsgraph.js",
    "tsgraph-generate": "bin/generate.js",
    "tsgraph-solve": "bin/solve.js"
  },
  "bugs": {
    "url": "https://github.com/kei-g/tsgraph/issues"
  },
  "dependencies": {
    "@kei-g/priority-queue": "^1.1.3",
    "canvas": "^2.9.0"
  },
  "description": "A star search algorithm implementation in TypeScript",
  "devDependencies": {
    "@kei-g/chmod": "^1.0.7",
    "@types/node": "^18.11.11",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "eslint": "^8.8.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "terser": "^5.10.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5"
  },
  "displayName": "tsgraph",
  "homepage": "https://github.com/kei-g/tsgraph",
  "keywords": [
    "A*",
    "A star",
    "Graph",
    "TypeScript"
  ],
  "license": "BSD-3-Clause",
  "main": "index.js",
  "name": "@kei-g/tsgraph",
  "publishConfig": {
    "access": "public"
  },
  "repository": "github:kei-g/tsgraph",
  "scripts": {
    "build": "npm-run-all -p clean lint -s build:tsc -p build:terse:* -s build:chmod",
    "build:chmod": "chmod +x bin/generate.js bin/solve.js tsgraph.js",
    "build:terse:astar": "terser build/lib/aStar.js -c -m -o lib/aStar.js --toplevel",
    "build:terse:common": "terser build/lib/common.js -c -m -o lib/common.js --toplevel",
    "build:terse:coordinate": "terser build/lib/coordinate.js -c -m -o lib/coordinate.js --toplevel",
    "build:terse:coordinate-unit": "terser build/lib/coordinateUnit.js -c -m -o lib/coordinateUnit.js --toplevel",
    "build:terse:euclidean": "terser build/lib/euclidean.js -c -m -o lib/euclidean.js --toplevel",
    "build:terse:generate": "terser build/bin/generate.js -c -m -o bin/generate.js --toplevel",
    "build:terse:graph": "terser build/lib/graph.js -c -m -o lib/graph.js --toplevel",
    "build:terse:index": "terser build/index.js -c -m -o index.js --toplevel",
    "build:terse:neighborhoods": "terser build/lib/neighborhoods.js -c -m -o lib/neighborhoods.js --toplevel",
    "build:terse:random": "terser build/lib/random.js -c -m -o lib/random.js --toplevel",
    "build:terse:solve": "terser build/bin/solve.js -c -m -o bin/solve.js --toplevel",
    "build:terse:tsgraph": "terser build/tsgraph.js -c -m -o tsgraph.js --toplevel",
    "build:terse:worker": "terser build/lib/worker.js -c -m -o lib/worker.js --toplevel",
    "build:tsc": "tsc",
    "clean": "rimraf bin/ build/ index.d.ts index.js lib/ tsgraph.d.ts tsgraph.js",
    "example": "ts-node src/example.ts",
    "generate": "ts-node src/bin/generate.ts",
    "lint": "eslint src/bin/*.ts src/lib/*.ts src/*.ts",
    "postpublish": "run-s clean",
    "prepublishOnly": "run-s build",
    "solve": "ts-node src/bin/solve.ts",
    "start": "run-s generate solve"
  },
  "types": "index.d.ts",
  "version": "1.2.0"
}
